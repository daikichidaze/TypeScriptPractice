{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch JavaScript",
      "program": "${workspaceFolder}/dist/*.js",
      "outFiles": ["${workspaceFolder}/dist/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch TypeScript",
      "preLaunchTask": "Compile TypeScript",
      "cwd": "${workspaceFolder}",
      "program": "${file}",
      "outFiles": ["${workspaceFolder}/dist/*.js"]
    }
  ]
}
